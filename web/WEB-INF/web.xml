<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <!--Servlet start-->
    <servlet>
        <servlet-name>ListServlet</servlet-name>
        <servlet-class>com.mango.im.servlet.ListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListServlet2</servlet-name>
        <servlet-class>com.mango.im.servlet.ListServlet_2_6</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteOneServlet</servlet-name>
        <servlet-class>com.mango.im.servlet.DeleteOneServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBatchServlet</servlet-name>
        <servlet-class>com.mango.im.servlet.DeleteBatchServlet</servlet-class>
    </servlet>
    <!--Servlet end-->

    <!--Servlet映射 start-->
    <servlet-mapping>
        <servlet-name>ListServlet</servlet-name>
        <url-pattern>/List.action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListServlet2</servlet-name>
        <url-pattern>/List2.action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteOneServlet</servlet-name>
        <url-pattern>/DeleteOneServlet.action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteBatchServlet</servlet-name>
        <url-pattern>/DeleteBatchServlet.action</url-pattern>
    </servlet-mapping>
    <!--Servlet映射 end-->

</web-app>